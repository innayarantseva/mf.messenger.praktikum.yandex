{
    "compilerOptions": {
        "allowJs": true,
        "outDir": "./dist",
        "target": "es5",
        "module": "esnext",
        "sourceMap": true,
        "allowUmdGlobalAccess": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "noUnusedLocals": true,
        // "noUnusedParameters": true
    },

    "include": [
        ".eslintrc.js",
        "./src/**/*"
    ],
    "exclude" : ["node_modules", "dist", "./src/**/*.spec.ts"]
}